<?xml version="1.0" encoding="UTF-8"?>
<style2tagmap xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:public::dita4publishers.org:namespaces:word2dita:style2tagmap style2tagmap.xsd"
  xmlns="urn:public:dita4publishers.org:namespaces:word2dita:style2tagmap"
  
  >
  <title>Default Word Styles to BookMap Style-to-Tag Map</title>
  <documentation xmlns:html="http://www.w3.org/1999/xhtml">
    <html:div xmlns="http://www.w3.org/1999/xhtml">
      <p>Defines a usable mapping from the built-in MS Word styles to
      DITA markup. Generates a bookmap and topics from a single input document.</p>
      <p>The First non-skipped paragraph in the document should styled as
      "Title", which it mapped to &lt;title&gt; within &lt;bookmap&gt; and triggers
      the generation of the root output map.</p>
    </html:div>
  </documentation>
  <styles>
    <output
      name="map"
      doctype-system="map.dtd"
      doctype-public="-//OASIS//DTD DITA Map//EN"
    />
    <output
      name="bookmap"
      doctype-system="bookmap.dtd"
      doctype-public="-//OASIS//DTD DITA BookMap//EN"
    />
    
    <output
      name="concept"
      doctype-system="concept.dtd"
      doctype-public="-//OASIS//DTD DITA Concept//EN"
    />
    
    <output
      name="task"
      doctype-system="task.dtd"
      doctype-public="-//OASIS//DTD DITA Task//EN"
    />
    
    <output
      name="reference"
      doctype-system="reference.dtd"
      doctype-public="-//OASIS//DTD DITA Reference//EN"
    />

    <style styleName="Title"
      level="0"
      format="bookmap"
      mapType="bookmap"
      prologType="topicmeta"
      structureType="mapTitle"
      tagName="title"
    />
    
    <style styleName="heading 1"
      structureType="topicTitle"
      topicDoc="yes"
      topicType="concept"
      bodyType="conbody"
      abstractType="abstract"
      shortdescType="shortdesc"
      prologType="prolog"
      initialSectionType="foosection"
      navtitleType="navtitle"
      rootTopicrefType="topicgroup"
      format="concept"
      topicrefType="chapter"
      tagName="title"
      level="1"
      maprefFormat="ditamap"
      mapFormat="map"
      maprefType="mapref"
      chunk="to-content"
    />    
    <style styleName="heading 2"
      structureType="topicTitle"
      topicDoc="yes"
      topicType="concept"
      bodyType="conbody"
      format="concept"
      topicrefType="topicref"
      tagName="title"
      level="2"
    />    
    
    
      <style styleName="heading 3"
      structureType="topicTitle"
      topicDoc="yes"
      topicType="concept"
      bodyType="conbody"
      format="concept"
      topicrefType="topicref"
      tagName="title"
      level="3"
      
    />    
    
    <style styleName="heading 4"
      structureType="topicTitle"
      topicDoc="yes"
      topicType="concept"
      bodyType="conbody"
      format="concept"
      topicrefType="topicref"
      tagName="title"
      level="4"
    />    
    
<style
  styleName="Normal"
  tagName="p"
  topicZone="body"
  level="1"
/>
    
    <style
      styleName="DefaultParagraph Font"
      tagName="ph"
      topicZone="body"
      level="1"/>
    
    <style
      styleName="Cover Page Name"
      tagName="p"
      topicZone="body"
      level="1"/>
    <style
      styleName="Cover Page Name Char"
      tagName="ph"
      topicZone="body"
      level="1"/>
    
    <style
      styleName="Heading 1 Char"
      tagName="ph"
      topicZone="body"
      level="1"/>
    <style
      styleName="Title Char"
      tagName="ph"
      topicZone="body"
      level="1"/>
    
    <style
      styleName="Heading 2 Char"
      tagName="ph"
      topicZone="body"
      level="1"/>
    
    <style
      styleName="TOC 1"      
      structureType="skip"/>
    <style
      styleName="TOC 2"
      structureType="skip"/>
    <style
      styleName="TOC 3"
      structureType="skip"/>
    <style
      styleName="TOC 4"
      structureType="skip"/>
    <style
      styleName="TOC 5"
      structureType="skip"/>
    <style
      styleName="TOC 6"
      structureType="skip"/>
    <style
      styleName="TOC 7"
      structureType="skip"/>
    <style
      styleName="TOC 8"
      structureType="skip"/>
    <style
      styleName="TOC 9"
      structureType="skip"/>
    
    <style
      styleName="Heading 3 Char"
      tagName="ph"
      topicZone="body"
      level="1"/>
    <style
      styleName="Heading 4 Char"
      tagName="ph"
      topicZone="body"
      level="1"/>
    
    <style
      styleName="List Bullet"
      containerType="ul"
      tagName="li"
      level="1"
      topicZone="body"/>
    <style styleName="Body Text Indent" 
      containerType="li" 
      idGenerator="default" 
      level="2" 
      tagName="p" 
      topicZone="body"/>
    <style
      styleName="Body Text Indent Char"
      tagName="ph"
      topicZone="body"
      level="1"/>
    <style
      styleName="List Bullet 2"
      containerType="ul"
      tagName="li"
      level="2"
      topicZone="body"/>
    <style
      styleName="Block Text"
      tagName="longquote"
      topicZone="body"
      level="1"/>
    <style
      styleName="Body Text"
      tagName="p"
      topicZone="body"
      level="1"/>
    <style
      styleName="BodyTextChar"
      tagName="ph"
      topicZone="body"
      level="1"/>
    <style
      styleName="List Number"
      containerType="ol"
      tagName="li"
      level="1"
      structureType="block"
      topicZone="body"/>
    <style
      styleName="List Number 2"
      containerType="ol"
      tagName="li"
      level="2"
      structureType="block"
      topicZone="body"/>
    
    
<style
  styleName="DefTerm"
  structureType="dt"
  tagName="dt"
  dlEntryType="dlentry"
  containerType="dl"
  topicZone="body"
/>
<style
  styleName="DefDesc"
  structureType="dd"
  tagName="dd"
  dlEntryType="dlentry"
  containerType="dl"
  topicZone="body"
/>
    
<style 
  styleName="My Section Title"
  structureType="section" 
  tagName="title"
  sectionType="section"
  topicZone="body"
  useAsTitle="yes"  
/>
    
<style 
  styleName="Lesson Title"
  structureType="topicTitle"
  initialSectionType="lcIntro"
  topicType="learningContent"
  bodyType="learningContentbody"
  level="1"
/>    
    
<style styleName="section"
  structureType="section"
  tagName="p"
  topicZone="body"
  spectitle="#toColon"
  
/>
    
    <style styleName="bold"
      topicZone="inline"
      tagName="b"
      />
    
  </styles>
</style2tagmap>